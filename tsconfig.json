{
  "compilerOptions": {
    "target": "esnext",
    "module": "esnext",
    "moduleResolution": "node",
    "strict": true,
    "importHelpers": true,
    "isolatedModules": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "experimentalDecorators": true,
    "lib": ["dom", "esnext"],
    "allowSyntheticDefaultImports": true,
    "types": [
      "unplugin-icons/types/vue",
    ]
  },
  "exclude": ["**/node_modules/**", "**/dist/**"]
}
